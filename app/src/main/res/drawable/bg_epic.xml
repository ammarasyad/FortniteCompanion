<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape android:shape="rectangle">
			<gradient
				android:angle="45"
				android:endColor="#BE56F8"
				android:startColor="#8B41CA" />
			<padding
				android:bottom="4dp"
				android:left="4dp"
				android:right="4dp"
